end
coroutine.wrap(EAESOUC_fake_script)()
local function ZCVK_fake_script() -- bootup.text 
	local script = Instance.new('LocalScript', bootup)

	while true do
		wait(.3)
		script.Parent.Text = "booting up or smthing."
		wait(.3)
		script.Parent.Text = "booting up or smthing.."
		wait(.3)
		script.Parent.Text = "booting up or smthing..."
	end
end
coroutine.wrap(ZCVK_fake_script)()
